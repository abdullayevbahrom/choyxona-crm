APP_URL=https://crm.example.com

APP_LOCALE=uz
APP_FALLBACK_LOCALE=uz
APP_FAKER_LOCALE=en_US
ALLOW_PUBLIC_REGISTRATION=false

APP_MAINTENANCE_DRIVER=cache
APP_MAINTENANCE_STORE=database

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
